{
  "compilerOptions": {
    "allowSyntheticDefaultImports": true,
    "jsx": "preserve",
    // "lib": ["dom", "esnext"],
    "lib": [
      "es6",
      "ES5",
      "ES7"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    // "skipLibCheck": true,
    // "resolveJsonModule": true,
    // "strict": false,
    "target": "esnext",
    "allowJs": true,
    "forceConsistentCasingInFileNames": true,
    "esModuleInterop": true,
    "module": "esnext",
    "isolatedModules": true,
    // "strict": true,
    // "types": [
    //   "node"
    // ],
    // "incremental": true
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "extends": "expo/tsconfig.base"
}
